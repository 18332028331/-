#define  _CRT_SECURE_NO_WARNINGS 1
#include<stdio.h>
int main()
{int day = 0;
scanf("%d", &day);
switch (day)//switch(整形表达式)
{
case 1://case(整型常量表达式）
	printf("星期一\n");
	break;
case 2:
	printf("星期二\n");
	break;
case 3:
	printf("星期三\n");
	break;
case 4:
	printf("星期四\n");
	break;
case 5:
	printf("星期五\n");
	break;
case 6:
	printf("星期六\n");
	break;
case 7:
	printf("星期天\n");
	break;
}
default:
break;
	return 0;
}
